{
  "name": "file-update-calendar",
  "displayName": "File Update Calendar",
  "description": "A VS Code extension to display file updates in a calendar",
  "version": "1.0.1",
  "publisher": "pab",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.showCalendar"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.showCalendar",
        "title": "Show File Update Calendar"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.56.0",
    "typescript": "^4.3.2"
  }
  
}

